// prisma/schema.prisma

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

generator client {
  provider = "prisma-client-js"
}

model Order {
  id        Int      @id @default(autoincrement())
  userId    String
  email     String
  total     Float
  items     Json
  createdAt DateTime @default(now())
}
